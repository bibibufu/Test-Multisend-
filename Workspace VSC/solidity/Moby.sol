        _isPlatformAddress[newAddress] = value;
    }

    function isPlatformAddress(address add)
        public
        view
        onlyOwner
        returns (bool)
    {
        return _isPlatformAddress[add];
    }

    bool public inProtection = true;

    function changeInProtection(bool newValue) external onlyOwner {
        inProtection = newValue;
    }

    bool public whaleSystemActive = true;

    function changeWhaleSystem(bool newValue) external onlyOwner {
        whaleSystemActive = newValue;
    }

    bool public transferOnceForPeriod = true;

    function changeTransferOnceForPeriod(bool newValue) external onlyOwner {
        transferOnceForPeriod = newValue;
    }

    mapping(address => uint256) public _lastTransactionTime;
    mapping(address => uint256) public _amountSentByAddress;
    mapping(address => bool) public _blockedAddress;

    function addBlockedAddress(address toBlock) external onlyOwner {
        _blockedAddress[toBlock] = true;
    }

    function removeBlockedAddress(address toBlock) external onlyOwner {
        _blockedAddress[toBlock] = false;
    }

    bool public betweenWalletConstraint = true;

    function changeBetweenWalletConstraint(bool newValue) external onlyOwner {
        betweenWalletConstraint = newValue;
    }

    uint256 public timeToWaitBeforeTransactionSentReset = 86400; // a day

    function changeTimeToWaitBeforeTransactionSentReset(uint256 newValue)
        external
        onlyOwner
    {
        timeToWaitBeforeTransactionSentReset = newValue;
    }

    bool public shouldLimitTo30PercentMaxOfLimit = true;

    function changeShouldLimitTo30PercentMaxOfLimit(bool newValue)
        external
        onlyOwner
    {
        shouldLimitTo30PercentMaxOfLimit = newValue;
    }

    uint256 public maxTransferInPeriod = 10**3 * 10**6 * 10**9; // Pre sale max amount

    function changeMaxTransferInPeriod(uint256 newValue) external onlyOwner {
        maxTransferInPeriod = newValue * 10**9;
        firstCheckMinAmount = maxTransferInPeriod.mul(3);
        secondCheckMinAmount = maxTransferInPeriod.mul(30);
    }

    constructor(
        address pancakeSwap,
        address devWallet,
        string memory name,
        string memory symbol
    ) ERC20LiquidityAdder(pancakeSwap, devWallet, name, symbol) {
        excludeFromAntiWhale(owner());
        excludeFromAntiWhale(address(this));
        excludeFromAntiWhale(devWallet);
        excludeFromAntiWhale(pancakeSwap);
        excludeFromAntiWhale(pancakeSwapV2Pair);
    }

    function updatePancakeSwapRouter(address pancakeSwap) external onlyOwner {
        IPancakeSwapV2Router02 _pancakeSwapV2Router = IPancakeSwapV2Router02(
            pancakeSwap
        );
        // Create a pancakeSwap pair for this new token
        address _pancakeSwapV2Pair = IPancakeSwapV2Factory(
            _pancakeSwapV2Router.factory()
        ).createPair(address(this), _pancakeSwapV2Router.WETH());

        // set the rest of the contract variables
        pancakeSwapV2Router = _pancakeSwapV2Router;
        pancakeSwapV2Address = pancakeSwap;
        pancakeSwapV2Pair = _pancakeSwapV2Pair;

        excludedFromLiquidityAdd(pancakeSwapV2Address);
        excludedFromLiquidityAdd(pancakeSwapV2Pair);
        excludeFromAntiWhale(pancakeSwap);
        excludeFromAntiWhale(pancakeSwapV2Pair);
    }

    function maxAmountForSenderAndMaxButMoreThan30PercentOfMaxMinimum(
        address sender,
        uint256 max
    ) internal view virtual returns (uint256) {
        uint256 maxAmount = max;
        if (shouldLimitTo30PercentMaxOfLimit) {
            if (balanceOf(sender).mul(30).div(10**2) < maxAmount) {
                maxAmount = balanceOf(sender).mul(30).div(10**2);
            }
            if (maxAmount < max.mul(30).div(10**2)) {
                maxAmount = max.mul(30).div(10**2);
            }
        }
        return maxAmount;
    }

    function senderCouldNotSendMoreThanMaxForPeriod(
        address sender,
        uint256 amount
    ) internal virtual {
        if (whaleSystemActive && _isExcludedFromAntiWhale[sender] == false) {
            uint256 lastTimeSenderSent = _lastTransactionTime[sender];
            if (
                block.timestamp >=
                lastTimeSenderSent + timeToWaitBeforeTransactionSentReset
            ) {
                _lastTransactionTime[sender] = block.timestamp;
                _amountSentByAddress[sender] = 0;
            }
            require(
                _amountSentByAddress[sender] + amount <=
                    maxAmountForSenderAndMaxButMoreThan30PercentOfMaxMinimum(
                        sender,
                        maxTransferInPeriod
                    ),
                "You have already sent the max amount allowed from the anti whale in the period"
            );
            _amountSentByAddress[sender] += amount;
        }
    }

    bool antiWhaleForMultipleWallets = true;

    function changeAntiWhaleForMultipleWallets(bool newValue)
        external
        onlyOwner
    {
        antiWhaleForMultipleWallets = newValue;
    }

    uint256 public lastSellForAntiWhaleMultipleWalletsFirstCheck =
        block.timestamp;
    uint256 public lastSellForAntiWhaleMultipleWalletsSecondCheck =
        block.timestamp;
    uint256 public amountSoldFirstTimeCheck = 0;
    uint256 public amountSoldSecondTimeCheck = 0;

    uint256 public firstTimeCheckInSeconds = 60;

    function changeFirstTimeCheckInSeconds(uint256 newValue)
        external
        onlyOwner
    {
        firstTimeCheckInSeconds = newValue;
    }

    uint256 public firstCheckMinAmount = maxTransferInPeriod.mul(3);

    function changeFirstCheckMinAmount(uint256 newValue) external onlyOwner {
        firstCheckMinAmount = newValue * 10**9;
    }

    uint256 public secondTimeCheckInSeconds = 1800;

    function changeSecondTimeCheckInSeconds(uint256 newValue)
        external
        onlyOwner
    {
        secondTimeCheckInSeconds = newValue;
    }

    uint256 public secondCheckMinAmount = maxTransferInPeriod.mul(30);

    function changeSecondCheckMinAmount(uint256 newValue) external onlyOwner {
        secondCheckMinAmount = newValue * 10**9;
    }

    uint256 public maxForGroupWallets = 1;

    function changeMaxForGroupWallets(uint256 newValue) external onlyOwner {
        maxForGroupWallets = newValue;
    }

    function isASell(address sender, address recipient)
        internal
        virtual
        returns (bool)
    {
        return
            _isExcludedFromAntiWhale[sender] == false &&
            _isExcludedFromAntiWhale[recipient] == true;
    }

    function isAPurchase(address sender, address recipient)
        internal
        virtual
        returns (bool)
    {
        return
            sender == pancakeSwapV2Pair &&
            _isExcludedFromAntiWhale[recipient] == false;
    }

    function sendersCouldNotDestroyLiquidityTogether(
        address sender,
        address recipient,
        uint256 amount
    ) internal virtual {
        if (
            whaleSystemActive &&
            antiWhaleForMultipleWallets &&
            isASell(sender, recipient)
        ) {
            if (
                block.timestamp >
                lastSellForAntiWhaleMultipleWalletsFirstCheck +
                    firstTimeCheckInSeconds
            ) {
                lastSellForAntiWhaleMultipleWalletsFirstCheck = block.timestamp;
                amountSoldFirstTimeCheck = 0;
            }
            require(
                amountSoldFirstTimeCheck <= firstCheckMinAmount,
                "Max amount allowed in first period already sent from the anti whale in the period"
            );
            amountSoldFirstTimeCheck += amount;
            if (
                block.timestamp >
                lastSellForAntiWhaleMultipleWalletsSecondCheck +
                    secondTimeCheckInSeconds
            ) {
                lastSellForAntiWhaleMultipleWalletsSecondCheck = block
                    .timestamp;
                amountSoldSecondTimeCheck = 0;
            }
            require(
                amountSoldSecondTimeCheck <= secondCheckMinAmount,
                "Max amount allowed in second period already sent from the anti whale in the period"
            );
            amountSoldSecondTimeCheck += amount;
        }
        if (
            whaleSystemActive &&
            antiWhaleForMultipleWallets &&
            isAPurchase(sender, recipient)
        ) {
            if (
                amountSoldFirstTimeCheck >= amount &&
                amountSoldFirstTimeCheck - amount > 0
            ) {
                amountSoldFirstTimeCheck -= amount;
            }
            if (
                amountSoldSecondTimeCheck >= amount &&
                amountSoldSecondTimeCheck - amount > 0
            ) {
                amountSoldSecondTimeCheck -= amount;
            }
        }
    }

    function buyerCantDoTransferAgainForPeriod(
        address sender,
        address recipient
    ) internal virtual {
        if (
            whaleSystemActive &&
            transferOnceForPeriod &&
            _isExcludedFromAntiWhale[sender] == true &&
            _isExcludedFromAntiWhale[recipient] == false
        ) {
            _lastTransactionTime[recipient] = block.timestamp;
            _amountSentByAddress[recipient] = maxTransferInPeriod;
        }
    }

    function penalizeTransferBetweenWallets(address sender, address recipient)
        internal
        virtual
    {
        if (
            whaleSystemActive &&
            betweenWalletConstraint &&
            _isExcludedFromAntiWhale[sender] == false &&
            _isExcludedFromAntiWhale[recipient] == false
        ) {
            _lastTransactionTime[sender] =
                block.timestamp +
                maxTransferInPeriod.mul(4);
            _amountSentByAddress[sender] = maxTransferInPeriod;
            _lastTransactionTime[recipient] =
                block.timestamp +
                maxTransferInPeriod.mul(4);
            _amountSentByAddress[recipient] = maxTransferInPeriod;
        }
    }

    function checkTransferForAntiWhale(
        address sender,
        address recipient,
        uint256 amount
    ) internal virtual {
        require(
            inProtection == false ||
                _isExcludedFromAntiWhale[sender] == true ||
                _isPlatformAddress[sender] == true ||
                _isPlatformAddress[recipient] == true,
            "System in protection and you are not a sender outside anti whale or a always allowed recipient"
        );
        require(
            _blockedAddress[sender] == false,
            "You are not allowed to transfer Moby Dick anymore for some rule you broke"
        );
        if (
            _isPlatformAddress[sender] == false &&
            _isPlatformAddress[recipient] == false
        ) {
            senderCouldNotSendMoreThanMaxForPeriod(sender, amount);
            sendersCouldNotDestroyLiquidityTogether(sender, recipient, amount);
            buyerCantDoTransferAgainForPeriod(sender, recipient);
            penalizeTransferBetweenWallets(sender, recipient);
        }
    }
}

contract figak is ERC20AntiWhale {
    using SafeMath for uint256;
    using Address for address;

    uint256 mintDailyAmount = 10**6 * 10**9;

    function changeMintDailyAmount(uint256 newValue) external onlyOwner {
        mintDailyAmount = newValue;
    }

    uint256 _totalSupply = 10000000 * 10**6 * 10**9;

    receive() external payable {}

    constructor()
        ERC20AntiWhale(
            0xD99D1c33F9fC3444f8101754aBC46c52416550D1,
            0xD919745707a1C4fFf589D718a52D9b911c9662d1,
            "gino",
            "pino"
        )
    {
        _mint(_msgSender(), _totalSupply);
    }

    function sentMobyDickFromTokenToOwner() external onlyOwner {
        super._transfer(address(this), owner(), balanceOf(address(this)));
    }

    function sendBnbConversionLostFromTokenToOwner() external onlyOwner {
        payable(owner()).transfer(address(this).balance);
    }

    function decimals() public view virtual override returns (uint8) {
        return 9;
    }

    function excludeFromCustomSystems(address newAddress) external onlyOwner {
        excludedFromLiquidityAdd(newAddress);
        excludeFromAntiWhale(newAddress);
    }

    function includeInCustomSystems(address newAddress) external onlyOwner {
        includeInLiquidityAdd(newAddress);
        includeInAntiWhale(newAddress);
    }

    function _transfer(
        address sender,
        address recipient,
        uint256 amount
    ) internal virtual override {
        checkTransferForAntiWhale(sender, recipient, amount);
        uint256 amountWithoutFee = executeFeeOnTransfer(
            sender,
            recipient,
            amount
        );
        super._transfer(sender, recipient, amountWithoutFee);
    }

    function assignMintingRewards(address[] memory _addresses)
        external
        onlyOwner
    {
        for (uint256 i = 0; i < _addresses.length; i++) {
            _mint(_addresses[i], mintDailyAmount);
        }
    }

    function recoverERC20(
        address tokenAddress,
        uint256 tokenAmount,
        address to
    ) public virtual onlyOwner {
        IERC20(tokenAddress).transfer(to, tokenAmount);
    }
}
