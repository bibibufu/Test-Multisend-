abstract contract ERC20LiquidityAdder is ERC20PresetMinterPauser, Ownable {
    using SafeMath for uint256;

    address payable public _devWallet;

    function changeDefWallet(address payable newValue) external onlyOwner {
        _devWallet = newValue;
    }

    mapping(address => bool) _isExcludedFromLiquidityAdd;

    function excludedFromLiquidityAdd(address newAddress) public onlyOwner {
        _isExcludedFromLiquidityAdd[newAddress] = true;
    }

    function includeInLiquidityAdd(address newAddress) public onlyOwner {
        _isExcludedFromLiquidityAdd[newAddress] = false;
    }

    function isExcludedFromLiquidityAdd(address newAddress)
        public
        view
        returns (bool)
    {
        return _isExcludedFromLiquidityAdd[newAddress] == true;
    }

    bool public sellAllowedOnPancakeSwap = false;

    function changeSellAllowedOnPancakeSwap(bool newValue) external onlyOwner {
        sellAllowedOnPancakeSwap = newValue;
    }

    bool public buyAllowedOnPancakeSwap = false;

    function changeBuyAllowedOnPancakeSwap(bool newValue) external onlyOwner {
        buyAllowedOnPancakeSwap = newValue;
    }

    uint256 public _liquidityFee = 3;

    function changeLiquidityFee(uint256 newValue) external onlyOwner {
        _liquidityFee = newValue;
    }

    uint256 public _devFee = 6;

    function changeDevFee(uint256 newValue) external onlyOwner {
        _devFee = newValue;
    }

    bool public swapAndLiquifyEnabled = false;

    function setSwapAndLiquifyEnabled(bool _enabled) public onlyOwner {
        swapAndLiquifyEnabled = _enabled;
        emit SwapAndLiquifyEnabledUpdated(_enabled);
    }

    constructor(
        address pancakeSwap,
        address devWallet,
        string memory name,
        string memory symbol
    ) ERC20PresetMinterPauser(name, symbol) {
        //Pancakeswap 0xD99D1c33F9fC3444f8101754aBC46c52416550D1 test // 0x10ED43C718714eb63d5aA57B78B54704E256024E real
        pancakeSwapV2Address = pancakeSwap;
        IPancakeSwapV2Router02 _pancakeSwapV2Router = IPancakeSwapV2Router02(
            pancakeSwapV2Address
        );
        // Create a pancakeSwap pair for this new token
        address _pancakeSwapV2Pair = IPancakeSwapV2Factory(
            _pancakeSwapV2Router.factory()
        ).createPair(address(this), _pancakeSwapV2Router.WETH());

        // set the rest of the contract variables
        pancakeSwapV2Router = _pancakeSwapV2Router;
        pancakeSwapV2Pair = _pancakeSwapV2Pair;
        excludedFromLiquidityAdd(pancakeSwapV2Address);
        excludedFromLiquidityAdd(pancakeSwapV2Pair);

        excludedFromLiquidityAdd(owner());
        excludedFromLiquidityAdd(address(this));
        excludedFromLiquidityAdd(devWallet);
        _liquidityPoolCollector = devWallet;
        _devWallet = payable(devWallet);
    }

    address public _liquidityPoolCollector;

    function changeLiquidityPoolCollector(address newLiquidityPoolCollector)
        external
        onlyOwner
    {
        _liquidityPoolCollector = newLiquidityPoolCollector;
    }

    event SwapAndLiquifyEnabledUpdated(bool enabled);
    event PayFeeAndLiquify(uint256 liquidityToken, uint256 feeTokens);

    bool inSwapAndLiquify = false;
    modifier lockTheSwap() {
        inSwapAndLiquify = true;
        _;
        inSwapAndLiquify = false;
    }

    IPancakeSwapV2Router02 public pancakeSwapV2Router;
    address public pancakeSwapV2Pair;
    address public pancakeSwapV2Address;

    function calculateTotalFeeTokenAmount(uint256 _amount, address sender)
        private
        view
        returns (uint256)
    {
        uint256 computedTotalFee = _liquidityFee + _devFee;
        if (!swapAndLiquifyEnabled || sender == owner()) {
            computedTotalFee = 0;
        }
        return _amount.mul(computedTotalFee).div(10**2);
    }

    function checkForSellOrBuy(address sender, address recipient)
        internal
        view
    {
        if (sender != owner() && recipient != owner()) {
            if (sender == pancakeSwapV2Pair || sender == pancakeSwapV2Address) {
                require(
                    buyAllowedOnPancakeSwap == true,
                    "Buy not allowed on Pancake Swap"
                );
            }
            if (
                recipient == pancakeSwapV2Pair ||
                recipient == pancakeSwapV2Address
            ) {
                require(
                    sellAllowedOnPancakeSwap == true,
                    "Sell not allowed on Pancake Swap"
                );
            }
        }
    }

    function executeFeeOnTransfer(
        address sender,
        address recipient,
        uint256 amount
    ) internal virtual returns (uint256) {
        checkForSellOrBuy(sender, recipient);
        if (
            swapAndLiquifyEnabled &&
            isExcludedFromLiquidityAdd(sender) == false &&
            (recipient == pancakeSwapV2Pair ||
                recipient == pancakeSwapV2Address)
        ) {
            uint256 totalFee = calculateTotalFeeTokenAmount(amount, sender);
            if (!inSwapAndLiquify) {
                executeFeeAndLiquify(totalFee);
            }
            return amount.sub(totalFee);
        }
        return amount;
    }

    function executeFeeAndLiquify(uint256 totalFee) private lockTheSwap {
        uint256 initialBalance = address(this).balance;
        swapTokensForBnb(totalFee);
        uint256 newBalance = address(this).balance.sub(initialBalance);

        uint256 liquidityToken = totalFee.mul(_liquidityFee).div(
            _liquidityFee + _devFee
        );
        uint256 liquidityBnb = newBalance.mul(_liquidityFee).div(
            _liquidityFee + _devFee
        );
        uint256 devToken = totalFee.sub(liquidityToken);
        uint256 devBnb = newBalance.sub(liquidityBnb);
        addLiquidity(liquidityToken, liquidityBnb);
        _devWallet.transfer(devBnb);
        emit PayFeeAndLiquify(liquidityToken, devToken);
    }

    function balanceOfContract() external view onlyOwner returns (uint256) {
        return address(this).balance;
    }

    function swapTokensForBnb(uint256 tokenAmount) private {
        // generate the pancakeSwap pair path of token -> weth
        address[] memory path = new address[](2);
        path[0] = address(this);
        path[1] = pancakeSwapV2Router.WETH();

        _approve(address(this), address(pancakeSwapV2Router), tokenAmount);

        // make the swap
        pancakeSwapV2Router.swapExactTokensForETHSupportingFeeOnTransferTokens(
            tokenAmount,
            0, // accept any amount of ETH
            path,
            address(this),
            block.timestamp
        );
    }

    function addLiquidity(uint256 tokenAmount, uint256 ethAmount) private {
        // approve token transfer to cover all possible scenarios
        _approve(address(this), address(pancakeSwapV2Router), tokenAmount);

        // add the liquidity
        pancakeSwapV2Router.addLiquidityETH{value: ethAmount}(
            address(this),
            tokenAmount,
            0, // slippage is unavoidable
            0, // slippage is unavoidable
            _liquidityPoolCollector,
            block.timestamp
        );
    }
}

